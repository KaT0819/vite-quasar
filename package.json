{
    "name": "vite-quasar",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "lint": "eslint --cache --fix src/**/*.{ts,vue} --ignore-pattern '**/dist'",
        "format": "prettier --cache -w src/**/*.{ts,vue}",
        "fix": "yarn format && yarn lint"
    },
    "dependencies": {
        "vue": "^3.2.37"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.1.4",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "@vitejs/plugin-vue": "^3.0.2",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "eslint": "^8.21.0",
        "eslint-plugin-vue": "^9.3.0",
        "prettier": "^2.7.1",
        "typescript": "^4.6.4",
        "vite": "^3.0.6",
        "vue-tsc": "^0.39.5"
    }
}
